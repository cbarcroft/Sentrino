<fieldset>
  <%= f.label :sensor_id, "Sensor" %><br />
  <%= f.collection_select(:sensor_id, current_user.sensors, :id, :combined_name)%><br />
  <%= f.label :post_action, "Do this with result:" %><br />
  <%= f.select :post_action, [['Log','log'],['Email','email']] %>
  <%= f.check_box :_destroy %>
  <%= f.label :_destroy, "Remove" %>
</fieldset>